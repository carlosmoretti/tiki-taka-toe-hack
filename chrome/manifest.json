{
    "name": "Tiki Taka Toe Helper",
    "version": "1.0.0",
    "description": "Helper para Tiki Taka Toe.",
    "manifest_version": 3,
    "author": "carlosmoretti",
    "permissions": [
        "scripting",
        "tabs",
        "<all_urls>"
    ],
    "host_permissions": [
        "*://*/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "index.html",
        "default_title": "Latest Covid Report"
    }
}